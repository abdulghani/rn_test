{
  "name": "rn_test",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone 11 Pro\"",
    "start": "react-native start",
    "postinstall": "npm run ios:podinstall",
    "ios:podinstall": "pod install --project-directory=ios/",
    "test:base": "node --expose-gc ./node_modules/.bin/jest --config ./jest.config.ts --runInBand --coverage --logHeapUsage",
    "test": "npm run test:base -- --silent --coverage --ci",
    "check:staged": "lint-staged --config ./lintstagedrc.json --concurrent false --verbose",
    "lint:all": "eslint . --ext .js,.jsx,.ts,.tsx --no-inline-config",
    "lint:changed": "eslint --no-inline-config --quiet",
    "format:all": "prettier . --config ./.prettierrc.json --write --ignore-unknown",
    "format:changed": "prettier --config ./.prettierrc.json --write --ignore-unknown",
    "prepare": "husky install"
  },
  "dependencies": {
    "native-base": "^3.3.3",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-svg": "^12.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@jest/types": "^27.4.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^27.4.0",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^27.4.6",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.2.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
